{
    "app" : {
        "background" : {
            "scripts" : ["launch.js"]
        }
    },
    "permissions" : [
        "storage",
        "webview"
    ],
    "icons" : {
        "16"  : "icons/16x.png",
        "48"  : "icons/48x.png",
        "128" : "icons/128x.png"
    },
    "manifest_version" : 2,
    "name" : "DevDocs Wrapper",
    "version" : "1.0.0",
    "description" : "A simple wrapper for the devdocs.io site"
}